# Robot Ip address (RIO address)
ROBOT_IP="10.80.20.2"

# Docker container runtime
# IF you want to utilize a nvidia gpu, 
# make sure you install nvidia-container-toolkit, and do
# RUNTIME="nvidia"
RUNTIME="nvidia"

# The MAP you want to load. Look at src/common/common_metadata for available options
MAP_FOLDER="PUTNAM_ENU_TTL_CSV"

# Which sim map you want to load.
MAP_NAME="Putnam"

# available: LD06, MID360, 
#            OAKD_PRO_W_PCL, OAKD_LITE_PCL, 
#            OAKD_PRO_W_YOLO, OAKD_LITE_YOLO, 
#            OAKD_PRO_W, OAKD_LITE
# IF you want to run multiple SENSORS do
# SENSORS=OAKD_W_PRO,LD06,OAKD_PRO_W
SENSORS=OAKD_LITE

# The Robot you are running. Can take the values of KIT_BOT and IAC_CAR right now
ROBOT_NAME="KIT_BOT"

# Parameter configuration you want to load. Can take the values of CHARGED_UP and CRESCENDO now
GAME_TYPE="CRESCENDO"

# Whether to use sim time or not
USE_SIM_TIME=False

# Whether to launch networktable bridge
LAUNCH_NETWORKTABLE=True

# Whether to launch foxglove bridge
LAUNCH_FOXGLOVE=True

# GHOST_CAR_CONTROLLER options
# AutonomousController
# ManualController
GHOST_CAR_CONTROLLER=AutonomousController
# Whether to launch ghost car or not
LAUNCH_GHOST_CAR=False

# Whether to launch perception or not
LAUNCH_PERCEPTION=False

# Whether to launch localization or not
LAUNCH_LOCALIZATION=True
